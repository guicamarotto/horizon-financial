{
  "ConnectionStrings": {
    "Postgres": "Host=postgres;Port=5432;Database=finance_lab;Username=postgres;Password=postgres"
  },
  "Kafka": {
    "BootstrapServers": "kafka:29092"
  },
  "RabbitMq": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/"
  },
  "Resilience": {
    "MaxAttempts": 5,
    "BaseDelayMs": 1000,
    "MaxJitterMs": 300
  },
  "LimitRules": {
    "AccountMaxLimit": 250000,
    "FailSymbols": [ "DLQ1" ]
  },
  "Otel": {
    "Endpoint": "http://jaeger:4317"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
